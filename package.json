{
  "name": "bara-react-icons",
  "license": "MIT",
  "description": "Bara React vector icons for cross-platform application!",
  "homepage": "https://barajs.dev",
  "repository": "https://github.com/barajs/bara-react-icons",
  "version": "1.1.0",
  "module": "es/index.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "es",
    "src/lib"
  ],
  "dependencies": {
    "bara": "2.3.1",
    "bara-react": "2.0.1",
    "react-native-vector-icons": "6.4.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.5",
    "@types/react": "16.8.12",
    "@types/react-dom": "16.9.17",
    "@types/react-native": "0.57.42",
    "@types/react-native-vector-icons": "6.4.0",
    "@typescript-eslint/typescript-estree": "1.6.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "2.1.8",
    "semantic-release": "15.13.3",
    "tslint": "5.15.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.1"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "scripts": {
    "compile": "rm -rf dist && yarn compile:tsc && yarn compile:tsc:es",
    "compile:tsc": "tsc -b ./tsconfig.lib.json",
    "compile:tsc:es": "tsc -b ./tsconfig.libes.json",
    "compile:watch": "tsc -b ./tsconfig.lib.json -w",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
